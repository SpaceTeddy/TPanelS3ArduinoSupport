{
  "name": "TPanelS3ArduinoSupport",
  "version": "0.0.1",
  "description": "Arduino support library for TPanel S3",
  "keywords": "tpanel, s3, esp32, display",
  "authors": [
    {
      "name": "Space Teddy",
      "email": "spaceteddy1@gmx.net"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SpaceTeddy/TPanelS3ArduinoSupport.git"
  },
  "license": "MIT",
  "frameworks": "arduino",
  "platforms": "espressif32",
  "dependencies": [
    {
      "owner": "lvgl",
      "name": "lvgl",
      "version": "9.2.2"
    },
    {
      "name": "TouchLib",
      "version": "https://github.com/mmMicky/TouchLib.git#v0.0.2"
    }
  ],
  "build": {
    "srcDir": ".",
    "includeDir": "src",
    "flags": [
      "-I lib/Arduino_GFX-1.4.6/src",
      "-I lib/tpanels3",
      "-I lib/XL95x5_Driver"
    ],
    "srcFilter": [
      "+<src/*.cpp>",
      "+<src/*.c>",
      "+<lib/Arduino_GFX-1.4.6/src/*.cpp>",
      "+<lib/Arduino_GFX-1.4.6/src/*.c>",
      "+<lib/Arduino_GFX-1.4.6/src/canvas/*.cpp>",
      "+<lib/Arduino_GFX-1.4.6/src/databus/*.cpp>",
      "+<lib/Arduino_GFX-1.4.6/src/display/*.cpp>",
      "+<lib/tpanels3/*.cpp>",
      "+<lib/tpanels3/*.c>",
      "+<lib/XL95x5_Driver/*.cpp>",
      "+<lib/XL95x5_Driver/*.c>"
    ]
  },
  "export": {
    "include": [
      "src",
      "lib",
      "examples",
      "library.json",
      "LICENSE",
      "README.md"
    ]
  }
}




